<?xml version="1.0"?>
<robot name="test_robot">
    <link name="base_link">
        <visual>
            <geometry>
                <box size="0.370 0.832 0.320"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0"/>
        </visual>
    </link>

    <link name="wheel_1">
        <visual>
            <geometry>
                <cylinder length=".085" radius="0.2155"/>
            </geometry>
            <origin rpy="0 1.57 0" xyz="0 0 -0"/>
        </visual>
    </link>

    <link name="wheel_2">
        <visual>
            <geometry>
                <cylinder length="0.085" radius="0.2155"/>
            </geometry>
            <origin rpy="0 1.57 0" xyz="-0 0 -0"/>
        </visual>
    </link>

    <link name="wheel_3">
        <visual>
            <geometry>
                <cylinder length="0.085" radius="0.131"/>
            </geometry>
            <origin rpy="0 1.57 0" xyz="-0.0 -0.0 -0.0"/>
        </visual>
    </link>

    <link name="wheel_4">
        <visual>
            <geometry>
                <cylinder length="0.085" radius="0.131"/>
            </geometry>
            <origin rpy="0 1.57 0" xyz="-0.0 -0.0 -0.0"/>
        </visual>
    </link>

    <joint name="joint_base_wheel1" type="continuous">
        <axis xyz="-1 0 0"/>
        <origin rpy="0 0 0" xyz="0.252 0.257 -0.050"/>
        <parent link="base_link"/>
        <child link="wheel_1"/>
    </joint>

    <joint name="joint_base_wheel2" type="continuous">
        <axis xyz="-1 0 0"/>
        <origin rpy="0 0 0" xyz="-0.252 0.257 -0.050"/>
        <parent link="base_link"/>
        <child link="wheel_2"/>
    </joint>

    <joint name="joint_base_wheel3" type="continuous">
        <axis xyz="-1 0 0"/>
        <origin rpy="0 0 0" xyz="0.252 -0.295 -0.120"/>
        <parent link="base_link"/>
        <child link="wheel_3"/>
    </joint>

    <joint name="joint_base_wheel4" type="continuous">
        <axis xyz="-1 0 0"/>
<!--        <origin rpy="0 0 0" xyz="-0.252 -0.295 -0.120"/>-->
        <origin rpy="0 0 0" xyz="-0.252 -0.295 -0.120"/>
        <parent link="base_link"/>
        <child link="wheel_4"/>
    </joint>
</robot>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".QuicabotInspectionResult"
    android:padding="5dp" android:background="@color/colorPrimary">


    <TextView
        android:id="@+id/resultPageTitle"
        android:layout_width="match_parent"
        android:layout_margin="5dp"
        android:text="All Inspection Result:"


        app:layout_constraintBottom_toTopOf="@id/resultTableCard"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/general_title" />
    <androidx.cardview.widget.CardView
        style="@style/insepction_result_content_card"
        app:cardCornerRadius="2dp"
        app:layout_constraintTop_toBottomOf="@id/resultPageTitle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:id="@+id/resultTableCard">

        <com.bin.david.form.core.SmartTable
            app:layout_constraintTop_toBottomOf="@id/resultPageTitle"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/resultTable"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            />


    </androidx.cardview.widget.CardView>






</androidx.constraintlayout.widget.ConstraintLayout>